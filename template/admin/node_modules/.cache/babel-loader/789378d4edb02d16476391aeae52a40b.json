     {\n              img: '',\n              info: [\n                {\n                  title: '标题',\n                  value: '今日推荐',\n                  tips: '选填，不超过4个字',\n                  max: 4,\n                },\n                {\n                  title: '链接',\n                  value: '',\n                  tips: '请输入链接',\n                  max: 100,\n                },\n              ],\n            },\n          ],\n        },\n        isShow: {\n          title: '是否显示背景色',\n          val: true,\n        },\n        // 背景颜色\n        bgColor: {\n          title: '背景颜色(渐变)',\n          default: [\n            {\n              item: '#F62C2C',\n            },\n            {\n              item: '#F96E29',\n            },\n          ],\n          color: [\n            {\n              item: '#F62C2C',\n            },\n            {\n              item: '#F96E29',\n            },\n          ],\n        },\n        dotColor: {\n          title: '指示器颜色',\n          default: [\n            {\n              item: '#fff',\n            },\n          ],\n          color: [\n            {\n              item: '#fff',\n            },\n          ],\n        },\n        // 左右间距\n        lrConfig: {\n          title: '左右边距',\n          val: 10,\n          min: 0,\n        },\n        // 页面间距\n        mbConfig: {\n          title: '页面间距',\n          val: 0,\n          min: 0,\n        },\n        // 轮播图点样式\n        docConfig: {\n          cname: 'swiper',\n          title: '指示器样式',\n          type: 0,\n          list: [\n            {\n              val: '圆形',\n              icon: 'iconDot',\n            },\n            {\n              val: '直线',\n              icon: 'iconSquarepoint',\n            },\n            {\n              val: '数字',\n              icon: 'iconshuzi',\n            },\n            {\n              val: '无指示器',\n              icon: 'iconjinyong',\n            },\n          ],\n        },\n        txtStyle: {\n          title: '指示器位置',\n        