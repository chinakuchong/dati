<div class=\"msg-wrapper\">\n        <div class=\"msg-item\">\n          <div class=\"item\"><span>用户昵称：</span>{{ orderDetail.userInfo.nickname }}</div>\n          <div class=\"item\"><span>收货人：</span>{{ orderDetail.orderInfo.real_name }}</div>\n        </div>\n        <div class=\"msg-item\">\n          <div class=\"item\"><span>联系电话：</span>{{ orderDetail.orderInfo.user_phone }}</div>\n          <div class=\"item\"><span>收货地址：</span>{{ orderDetail.orderInfo.user_address }}</div>\n        </div>\n      </div>\n    </div>\n    <div class=\"msg-box\" style=\"border: none\">\n      <div class=\"box-title\">订单信息</div>\n      <div class=\"msg-wrapper\">\n        <div class=\"msg-item\">\n          <div class=\"item\"><span>订单ID：</span>{{ orderDetail.orderInfo.order_id }}</div>\n          <div class=\"item\" style=\"color: red\">\n            <span style=\"color: red\">订单状态：</span>{{ orderDetail.orderInfo._status._title }}\n          </div>\n        </div>\n        <div class=\"msg-item\">\n          <div class=\"item\"><span>商品总数：</span>{{ orderDetail.orderInfo.total_num }}</div>\n          <div class=\"item\">\n            <span>商品总价：</span\n            >{{ parseFloat(orderDetail.orderInfo.total_price) + parseFloat(orderDetail.orderInfo.vip_true_price || 0) }}\n          </div>\n        </div>\n        <div class=\"msg-item\">\n          <div class=\"item\"><span>交付邮费：</span>{{ orderDetail.orderInfo.pay_postage }}</div>\n          <div class=\"item\"><span>优惠券金额：</span>{{ orderDetail.orderInfo.coupon_price }}</div>\n        </div>\n        <div class=\"msg-item\">\n          <div class=\"item\"><span>实际支付：</span>{{ orderDetail.orderInfo.pay_price }}</div>\n          <div class=\"item\"><span>创建时间：</span>{{ orderDetail.orderInfo.add_time }}</div>\n        </div>\n        <div class=\"msg-item\">\n          <div class=\"item\"><span>支付方式：</span>{{ orderDetail.orderInfo._status._payType }}</div>\n          <div class=\"item\"><span>推广人：</span>{{ orderDetail.userInfo.spread_name }}</div>\n        </div>\n        <div class=\"msg-item\">\n          <div class=\"item\"><span>商家备注：</span>{{ orderDetail.orderInfo.mark }}</div>\n        </div>\n      </div>\n    </div>\n    <div class=\"goods-box\">\n      <Table :columns=\"columns1\" :data=\"orderList\">\n        <template slot-scope=\"{ row, index }\" slot=\"id\">\n          {{ row.productInfo.id }}\n        </template>\n        <template slot-scope=\"{ row, index }\" slot=\"name\">\n          <div class=\"product_info\">\n            <img :src=\"row.productInfo.image\" alt=\"\" />\n            <p>{{ row.productInfo.store_name }}</p>\n          </div>\n        </template>\n        <template slot-scope=\"{ row, index }\" slot=\"className\">\n          {{ row.class_name }}\n        </template>\n        <template slot-scope=\"{ row, index }\" slot=\"price\">\n          {{ row.productInfo.attrInfo.price }}\n        </template>\n        <template slot-scope=\"{ row, index }\" slot=\"total_num\">\n          {{ row.cart_num }}\n        </template>\n      </Table>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { orderInfo } from '@/api/kefu';\nexport default {\n  name: 'order_detail',\n  props: {\n    orderId: {\n      type: String | Number,\n      default: '',\n    },\n  },\n  data() {\n    return {\n      orderDetail: {},\n      orderList: [],\n      columns1: [\n        {\n          title: '商品ID',\n          slot: 'id',\n          maxWidth: 80,\n        },\n        {\n          title: '商品名称',\n          slot: 'name',\n          minWidth: 160,\n        },\n        {\n          title: '商品分类',\n          slot: 'className',\n        },\n        {\n          title: '商品售价',\n          slot: 'price',\n        },\n        {\n          title: '商品数量',\n          slot: 'total_num',\n        },\n      ],\n    };\n  },\n  mounted() {\n    this.getOrderInfo();\n  },\n  methods: {\n    getOrderInfo() {\n      orderInfo(this.orderId).then((res) => {\n        res.data.orderInfo.add_time = this.$moment(parseInt(res.data.orderInfo.add_time) * 1000).format('YYYY-MM-DD');\n        this.orderDetail = res.data;\n        this.orderList = res.data.orderInfo.cartInfo;\n      });\n    },\n  },\n};\n</script>\n\n<style lang=\"stylus\" scoped>\n.order_detail\n    .msg-box\n        border-bottom 1px solid #E8EAED\n        .box-title\n            padding-top 20px\n            font-size 16px\n            color #333\n        .msg-wrapper\n            margin-top 15px\n            padding-bottom 10px\n            .msg-item\n                display flex\n                .item\n                    flex 1\n                    margin-bottom 15px\n                    span\n                        color #333\n        &:first-child .box-title\n            padding-top 0\n    .product_info\n        display flex\n        align-items center\n        img\n            width 36px\n            height 36px\n            border-radius 4px\n            margin-right 10px\n</style>\n"],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA0EA,SAAAA,SAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACAC,OAAA;MACAC,IAAA,EAAAC,MAAA,GAAAC,MAAA;MACAC,OAAA;IACA;EACA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,WAAA;MACAC,SAAA;MACAC,QAAA,GACA;QACAC,KAAA;QACAC,IAAA;QACAC,QAAA;MACA,GACA;QACAF,KAAA;QACAC,IAAA;QACAE,QAAA;MACA,GACA;QACAH,KAAA;QACAC,IAAA;MACA,GACA;QACAD,KAAA;QACAC,IAAA;MACA,GACA;QACAD,KAAA;QACAC,IAAA;MACA;IAEA;EACA;EACAG,OAAA,WAAAA,QAAA;IACA,KAAAC,YAAA;EACA;EACAC,OAAA;IACAD,YAAA,WAAAA,aAAA;MAAA,IAAAE,KAAA;MACAnB,SAAA,MAAAG,OAAA,EAAAiB,IAAA,WAAAC,GAAA;QACAA,GAAA,CAAAb,IAAA,CAAAR,SAAA,CAAAsB,QAAA,GAAAH,KAAA,CAAAI,OAAA,CAAAC,QAAA,CAAAH,GAAA,CAAAb,IAAA,CAAAR,SAAA,CAAAsB,QAAA,UAAAG,MAAA;QACAN,KAAA,CAAAV,WAAA,GAAAY,GAAA,CAAAb,IAAA;QACAW,KAAA,CAAAT,SAAA,GAAAW,GAAA,CAAAb,IAAA,CAAAR,SAAA,CAAA0B,QAAA;MACA;IACA;EACA;AACA"}]}