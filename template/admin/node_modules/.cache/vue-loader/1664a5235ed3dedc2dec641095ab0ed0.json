;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"c_home_menu.vue","sourceRoot":"src/components/mobileConfigDiy","sourcesContent":["<template>\n  <div class=\"mobile-config pro\">\n    <div v-for=\"(item, key) in rCom\" :key=\"key\">\n      <component\n        :is=\"item.components.name\"\n        :configObj=\"configObj\"\n        ref=\"childData\"\n        :configNme=\"item.configNme\"\n        :key=\"key\"\n        @getConfig=\"getConfig\"\n        :index=\"activeIndex\"\n        :num=\"item.num\"\n      ></component>\n    </div>\n    <rightBtn :activeIndex=\"activeIndex\" :configObj=\"configObj\"></rightBtn>\n  </div>\n</template>\n\n<script>\nimport toolCom from '@/components/mobileConfigRightDiy/index.js';\nimport { mapState, mapMutations, mapActions } from 'vuex';\nimport rightBtn from '@/components/rightBtn/index.vue';\nexport default {\n  name: 'c_home_menu',\n  cname: '导航组',\n  componentsName: 'home_menu',\n  props: {\n    activeIndex: {\n      type: null,\n    },\n    num: {\n      type: null,\n    },\n    index: {\n      type: null,\n    },\n  },\n  components: {\n    ...toolCom,\n    rightBtn,\n  },\n  data() {\n    return {\n      configObj: {},\n      rCom: [\n        {\n          components: toolCom.c_set_up,\n          configNme: 'setUp',\n        },\n      ],\n      space: [\n        {\n          components: toolCom.c_menu_list,\n          configNme: 'menuConfig',\n        },\n      ],\n      space2: [],\n      oneStyle: [\n        {\n          components: toolCom.c_tab,\n          configNme: 'tabConfig',\n        },\n        {\n          components: toolCom.c_txt_tab,\n          configNme: 'menuStyle',\n        },\n        {\n          components: toolCom.c_bg_color,\n          configNme: 'bgColor',\n        },\n        {\n          components: toolCom.c_bg_color,\n          configNme: 'titleColor',\n        },\n        {\n          components: toolCom.c_txt_tab,\n          configNme: 'bgStyle',\n        },\n        {\n          components: toolCom.c_slider,\n          configNme: 'prConfig',\n        },\n        {\n          components: toolCom.c_slider,\n          configNme: 'mbConfig',\n        },\n      ],\n      twoStyle: [\n        {\n          components: toolCom.c_tab,\n          configNme: 'tabConfig',\n        },\n        {\n          components: toolCom.c_txt_tab,\n          configNme: 'menuStyle',\n        },\n        {\n          components: toolCom.c_txt_tab,\n          configNme: 'rowsNum',\n        },\n        {\n          components: toolCom.c_txt_tab,\n          configNme: 'number',\n        },\n        {\n          components: toolCom.c_txt_tab,\n          configNme: 'pointerStyle',\n        },\n        {\n          components: toolCom.c_bg_color,\n          configNme: 'pointerColor',\n        },\n        {\n          components: toolCom.c_bg_color,\n          configNme: 'bgColor',\n        },\n        {\n          components: toolCom.c_bg_color,\n          configNme: 'titleColor',\n        },\n        {\n          components: toolCom.c_txt_tab,\n          configNme: 'bgStyle',\n        },\n        {\n          components: toolCom.c_slider,\n          configNme: 'prConfig',\n        },\n        {\n          components: toolCom.c_slider,\n          configNme: 'mbConfig',\n        },\n      ],\n      type: 0,\n      setUp: 0,\n    };\n  },\n  watch: {\n    num(nVal) {\n      // debugger;\n      let value = JSON.parse(JSON.stringify(this.$store.state.mobildConfig.defaultArray[nVal]));\n      this.configObj = value;\n    },\n    configObj: {\n      handler(nVal, oVal) {\n        this.$store.commit('mobildConfig/UPDATEARR', { num: this.num, val: nVal });\n      },\n      deep: true,\n    },\n    'configObj.setUp.tabVal': {\n      handler(nVal, oVal) {\n        this.setUp = nVal;\n        var arr = [this.rCom[0]];\n        if (nVal == 0) {\n          let tempArr = [\n            {\n              components: toolCom.c_menu_list,\n              configNme: 'menuConfig',\n            },\n          ];\n          this.rCom = arr.concat(tempArr);\n        } else {\n          if (this.type) {\n            this.rCom = arr.concat(this.twoStyle);\n          } else {\n            this.rCom = arr.concat(this.oneStyle);\n          }\n        }\n      },\n      deep: true,\n    },\n    'configObj.tabConfig.tabVal': {\n      handler(nVal, oVal) {\n        this.type = nVal;\n        var arr = [this.rCom[0]];\n        if (this.setUp) {\n          if (nVal == 0) {\n            this.rCom = arr.concat(this.oneStyle);\n          } else {\n            this.rCom = arr.concat(this.twoStyle);\n          }\n        }\n      },\n      deep: true,\n    },\n  },\n  mounted() {\n    this.$nextTick(() => {\n      let value = JSON.parse(JSON.stringify(this.$store.state.mobildConfig.defaultArray[this.num]));\n      this.configObj = value;\n    });\n  },\n  methods: {\n    getConfig(data) {},\n  },\n};\n</script>\n\n<style scoped></style>\n"]}]}