liderChange\">\n          <Option v-for=\"(item, index) in datas[name].list\" :value=\"item.activeValue\" :key=\"index\">{{\n            item.title\n          }}</Option>\n        </Select>\n      </Col>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'c_select',\n  props: {\n    name: {\n      type: String,\n    },\n    configData: {\n      type: null,\n    },\n    configNum: {\n      type: Number | String,\n      default: 'default',\n    },\n  },\n  data() {\n    return {\n      defaults: {},\n      datas: this.configData[this.configNum],\n    };\n  },\n  mounted() {\n    if (this.name === 'selectConfig') {\n      this.bus.$on('upData', (data) => {\n        this.datas[this.name].list = data;\n        this.bus.$off();\n      });\n    }\n  },\n  watch: {\n    configData: {\n      handler(nVal, oVal) {\n        this.datas = nVal[this.configNum];\n      },\n      deep: true,\n    },\n  },\n  methods: {\n    sliderChange(e) {\n      this.$emit('getConfig', { name: 'select', values: e });\n    },\n  },\n};\n</script>\n\n<style scoped>\n.slider-box {\n  margin-top: 10px;\n}\n</style>\n"]}]}