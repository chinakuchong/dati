      <Col class=\"label\" span=\"4\">\n        {{ configData.title }}\n      </Col>\n      <Col span=\"19\" class=\"slider-box\">\n        <div @click=\"getLink(configData.title)\">\n          <Input\n            :icon=\"configData.title == '链接' ? 'ios-arrow-forward' : ''\"\n            :readonly=\"configData.title == '链接' ? true : false\"\n            v-model=\"configData.value\"\n            :placeholder=\"configData.place\"\n            :maxlength=\"configData.max\"\n          />\n        </div>\n      </Col>\n    </div>\n    <linkaddress ref=\"linkaddres\" @linkUrl=\"linkUrl\"></linkaddress>\n  </div>\n</template>\n\n<script>\nimport linkaddress from '@/components/linkaddress';\nexport default {\n  name: 'c_input_item',\n  props: {\n    configObj: {\n      type: Object,\n    },\n    configNme: {\n      type: String,\n    },\n  },\n  components: {\n    linkaddress,\n  },\n  data() {\n    return {\n      value: '',\n      defaults: {},\n      configData: {},\n    };\n  },\n  created() {\n    this.defaults = this.configObj;\n    this.configData = this.configObj[this.configNme];\n  },\n  watch: {\n    configObj: {\n      handler(nVal, oVal) {\n        this.defaults = nVal;\n        this.configData = nVal[this.configNme];\n      },\n      immediate: true,\n      deep: true,\n    },\n  },\n  methods: {\n    linkUrl(e) {\n      this.configData.value = e;\n    },\n    getLink(title) {\n      if (title != '链接') {\n        return;\n      }\n      this.$refs.linkaddres.modals = true;\n    },\n  },\n};\n</script>\n\n<style scoped lang=\"stylus\">\n/deep/.ivu-input\n    font-size 13px!important\n.c_row-item\n    margin-bottom 13px\n</style>\n"]}]}