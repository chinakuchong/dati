el=\"configData.status\" @on-change=\"change\" />\n      </Col>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'c_status',\n  props: {\n    configObj: {\n      type: Object,\n    },\n    configNme: {\n      type: String,\n    },\n  },\n  data() {\n    return {\n      configData: {\n        status: false,\n      },\n    };\n  },\n  created() {\n    this.defaults = this.configObj;\n    this.configData = this.configObj[this.configNme];\n  },\n  watch: {\n    configObj: {\n      handler(nVal, oVal) {\n        this.defaults = nVal;\n        this.configData = nVal[this.configNme];\n      },\n      immediate: true,\n      deep: true,\n    },\n  },\n  methods: {\n    change(status) {\n      this.$nextTick(() => {\n        this.configData.status = status;\n        this.$store.commit('mobildConfig/footStatus', status);\n      });\n      //   this.$emit(\"getConfig\", this.configData);\n    },\n  },\n};\n</script>\n\n<style scoped lang=\"stylus\">\n.c_row-item {\n  margin-top: 10px;\n  margin-bottom: 10px;\n}\n\n.color-box {\n  display: flex;\n  align-items: center;\n  justify-content: flex-end;\n\n  .color-item {\n    margin-left: 15px;\n\n    span {\n      margin-left: 5px;\n      color: #999;\n      font-size: 13px;\n      cursor: pointer;\n    }\n  }\n}\n</style>\n"]}]}